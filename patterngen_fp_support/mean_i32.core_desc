InstructionSet XExample extends RISCVBase {
    instructions {
        MEAN {
            encoding: 7'b0101000 :: rs2[4:0] :: rs1[4:0] :: 3'b011 :: rd[4:0] :: 7'b0101011;
            assembly: "{name(rd)}, {name(rs1)}, {name(rs2)}";
            behavior: {
                if (rd != 0) {
                    X[rd] = (X[rs1] + X[rs2]) / 2;
                }
            }
        }
    }
}
